public static void miniMaxSum(List<Integer> arr) {
    long[] ar = new long[arr.size()];
    int size = arr.size();
    for(int i = 0; i < size; i++){
        for(int k = 0; k < size; k++){
            if(k == i)
                continue;
            ar[i] += arr.get(k);
        }
    }
    Arrays.sort(ar);
    System.out.print(ar[0] + " ");
    System.out.println(ar[size-1]);
}
